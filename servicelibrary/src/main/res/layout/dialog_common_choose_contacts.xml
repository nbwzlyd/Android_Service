<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="300dp"
    android:layout_height="wrap_content"
    android:layout_centerInParent="true"
    android:background="@drawable/bg_list_window_rest"
    android:orientation="vertical"
    >
    <TextView
        style="@style/tv_list_secondary_style"
        android:layout_height="@dimen/vacation_cell_height"
        android:layout_width="fill_parent"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/common_margin"
        android:paddingRight="@dimen/common_margin"
        android:text="选择"
        android:background="@drawable/bg_common_expanderbg_line"
        />
    <TextView
        android:id="@+id/tv_common_contact"
        style="@style/tv_list_primary_style"
        android:layout_height="@dimen/vacation_cell_height"
        android:layout_width="fill_parent"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/common_margin"
        android:paddingRight="@dimen/common_margin"
        android:text="常用旅客"
        android:background="@drawable/selector_cell_single_line"/>
    <TextView
        android:id="@+id/tv_mobile_contact"
        style="@style/tv_list_primary_style"
        android:layout_height="@dimen/vacation_cell_height"
        android:layout_width="fill_parent"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/common_margin"
        android:paddingRight="@dimen/common_margin"
        android:text="手机通讯录"
        android:background="@drawable/selector_cell_white"/>

</LinearLayout>