<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="calendar_cell">
        <attr name="state_selectable" format="boolean" />
        <attr name="state_current_month" format="boolean" />
        <attr name="state_today" format="boolean" />
    </declare-styleable>
    <declare-styleable name="PullToRefresh">
        <attr name="adapterViewBackground" format="reference|color" />
        <attr name="pullheaderBackground" format="reference|color" />
        <attr name="headerTextColor" format="color" />
        <attr name="pullmode">
            <flag name="pullDownFromTop" value="0x1" />
            <flag name="pullUpFromBottom" value="0x2" />
            <flag name="both" value="0x3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="PullScrollView">
        <attr name="header" format="reference" />
        <attr name="headerHeight" format="dimension" />
        <attr name="headerVisibleHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="EditText">
 
        <!-- 总共输入长度 -->
        <attr name="totalLength" format="integer" />
        <!-- 每组的长度 -->
        <attr name="eachLength" format="integer" />
        <!-- 分隔符 -->
        <attr name="delimiter" format="string" />
        <!-- 占位符 -->
        <attr name="placeHolder" format="string" />
    </declare-styleable>
    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="indicatorColor" format="color" />
        <attr name="underlineColor" format="color" />
        <attr name="dividerColor" format="color" />
        <attr name="indicatorHeight" format="dimension" />
        <attr name="underlineHeight" format="dimension" />
        <attr name="dividerPadding" format="dimension" />
        <attr name="tabPaddingLeftRight" format="dimension" />
        <attr name="scrollOffset" format="dimension" />
        <attr name="tabBackground" format="reference" />
        <attr name="shouldExpand" format="boolean" />
    </declare-styleable>
    <declare-styleable name="ViewPagerIndicator">
        <attr name="vpiCirclePageIndicatorStyle" format="reference" />
        <attr name="vpiTabPageIndicatorStyle" format="reference" />
    </declare-styleable>

    <attr name="centered" format="boolean" />
    <attr name="selectedColor" format="color" />
    <attr name="strokeWidth" format="dimension" />
    <attr name="unselectedColor" format="color" />

    <declare-styleable name="CirclePageIndicator">

        <!-- Whether or not the indicators should be centered. -->
        <attr name="centered" />
        <!-- Color of the filled circle that represents the current page. -->
        <attr name="fillColor" format="color" />
        <!-- Color of the filled circles that represents pages. -->
        <attr name="pageColor" format="color" />
        <!-- Orientation of the indicator. -->
        <attr name="android:orientation" />
        <!-- Radius of the circles. This is also the spacing between circles. -->
        <attr name="radius" format="dimension" />
        <!-- Whether or not the selected indicator snaps to the circles. -->
        <attr name="snap" format="boolean" />
        <!-- Color of the open circles. -->
        <attr name="strokeColor" format="color" />
        <!-- Width of the stroke used to draw the circles. -->
        <attr name="strokeWidth" />
        <!-- View background -->
        <attr name="android:background" />
    </declare-styleable>

    <!-- 下拉扩展控件 -->
    <declare-styleable name="ExpandDownView">

        <!-- 是否可下拉扩展 -->
        <attr name="expandAble" format="boolean" />
        <!-- 是否多选 -->
        <attr name="multipleChoiceAble" format="boolean" />
        <!-- 是否有不限选项 -->
        <attr name="has_no_limit" format="boolean" />
        <!-- 控件默认样式 -->
        <attr name="style">

            <!-- 样式1：筛选样式 -->
            <flag name="filter" value="0x1" />
            <!-- 样式2：点评样式 -->
            <flag name="comment" value="0x2" />
        </attr>
        <!-- 当style指定filter样式时，设置标题 -->
        <attr name="filter_title" format="string" />
    </declare-styleable>

    <attr name="direction">
        <enum name="rightToLeft" value="0" />
        <enum name="bottomToTop" value="1" />
        <enum name="leftToRight" value="2" />
        <enum name="topToBottom" value="3" />
    </attr>

    <declare-styleable name="MultiDirectionSlidingDrawer">
        <attr name="handle" format="reference" />
        <attr name="content" format="reference" />
        <attr name="direction" />
        <attr name="bottomOffset" format="dimension" />
        <attr name="topOffset" format="dimension" />
        <attr name="allowSingleTap" format="boolean" />
        <attr name="animateOnClick" format="boolean" />
    </declare-styleable>

    <!-- rounded ImageView -->
    <declare-styleable name="RoundedImageView">
        <attr name="corner_radius" format="dimension" />
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="mutate_background" format="boolean" />
        <attr name="oval" format="boolean" />
        <attr name="android:scaleType" />
    </declare-styleable>

    <!-- tab page indicator attrs -->
    <declare-styleable name="AnimateTabPageIndicator">
        <attr name="textColor" format="reference|color" />
        <attr name="textSize" format="dimension" />
        <attr name="tabIndicatorSelectedColor" format="color" />
        <attr name="selectedHeight" format="dimension" />
        <attr name="unSelectedHeight" format="dimension"></attr>
        <attr name="tabIndicatorUnSelectedColor" format="color" />
        <attr name="paddingLeft" format="dimension" />
        <attr name="paddingRight" format="dimension" />
        <attr name="paddingTop" format="dimension" />
        <attr name="paddingBottom" format="dimension" />
    </declare-styleable>
    <declare-styleable name="TCLabelGroup">

        <!-- row margin 数据项 -->
        <attr name="rowMarginLeft" format="dimension " />
        <attr name="rowMarginRight" format="dimension" />
        <attr name="rowMarginTop" format="dimension" />
        <attr name="rowMarginBottom" format="dimension" />

        <!-- label margin 数据项 -->
        <attr name="labelMarginLeft" format="dimension" />
        <attr name="labelMarginRight" format="dimension" />
        <attr name="labelMarginTop" format="dimension" />
        <attr name="labelMarginBottom" format="dimension" />

        <!-- label padding 数据项 -->
        <attr name="labelPaddingLeft" format="dimension" />
        <attr name="labelPaddingRight" format="dimension" />
        <attr name="labelPaddingTop" format="dimension" />
        <attr name="labelPaddingBottom" format="dimension" />

        <!-- label 特有数据项 -->
        <attr name="labelRadioX" format="dimension" />
        <attr name="labelRadioY" format="dimension" />
        <attr name="labelTextSize" format="dimension" />
        <attr name="labelTextColor" format="color" />
        <attr name="labelBackgroundColor" format="color" />
    </declare-styleable>
    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="roundProgressTailColor" format="color" />
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="roundtextColor" format="color" />
        <attr name="roundtextSize" format="dimension" />
        <attr name="text" format="string" />
        <attr name="max" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="roundstyle">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>
    <!-- 价格滑动条 -->
    <declare-styleable name="RangeBar">
        <attr name="tickCount" format="integer" />
        <attr name="tickHeight" format="dimension" />
        <attr name="barWeight" format="dimension" />
        <attr name="barColor" format="reference|color" />
        <attr name="connectingLineWeight" format="dimension" />
        <attr name="connectingLineColor" format="reference|color" />
        <attr name="thumbRadius" format="dimension" />
        <attr name="thumbImageNormal" format="reference" />
        <attr name="thumbImagePressed" format="reference" />
        <attr name="thumbColorNormal" format="reference|color" />
        <attr name="thumbColorPressed" format="reference|color" />
    </declare-styleable>
    <declare-styleable name="ItemView">

        <!-- 左图标，不设置不显示 -->
        <attr name="itemDrawableLeft" format="reference" />
        <!-- 左图标宽度， 不设置自动适配图片宽度 -->
        <attr name="itemDrawableLeftWidth" format="dimension" />
        <!-- 左图标高度， 不设置自动适配图片高度 -->
        <attr name="itemDrawableLeftHeight" format="dimension" />
        <!-- 左图标与做文字填充距离， 无左图标此数值无效 -->
        <attr name="itemDrawableLeftPadding" format="dimension" />
        <!-- 右图标，不设置不显示 -->
        <attr name="itemDrawableRight" format="reference" />
        <!-- 右图标宽度， 不设置自动适配图片宽度 -->
        <attr name="itemDrawableRightWidth" format="dimension" />
        <!-- 右图标高度， 不设置自动适配图片高度 -->
        <attr name="itemDrawableRightHeight" format="dimension" />
        <!-- 右图标与做文字填充距离， 无右图标此数值无效 -->
        <attr name="itemDrawableRightPadding" format="dimension" />
        <!-- 左文字，不设置则显示hint -->
        <attr name="itemTextLeft" format="string|reference" />
        <!-- 左文字大小，默认15sp -->
        <attr name="itemTextLeftSize" format="dimension" />
        <!-- 左文字颜色，默认black -->
        <attr name="itemTextLeftColor" format="reference|color" />
        <!-- 左文字hint -->
        <attr name="itemTextLeftHint" format="string|reference" />
        <!-- 左文字hint颜色，默认gray -->
        <attr name="itemTextLeftHintColor" format="reference|color" />
        <!-- 左文字hint大小，默认同左文字大小 -->
        <attr name="itemTextLeftHintSize" format="dimension" />
        <!-- 右文字，不设置则显示hint -->
        <attr name="itemTextRight" format="string|reference" />
        <!-- 右文字大小，默认15sp -->
        <attr name="itemTextRightSize" format="dimension" />
        <!-- 右文字颜色，默认black -->
        <attr name="itemTextRightColor" format="reference|color" />
        <!-- 右文字hint -->
        <attr name="itemTextRightHint" format="string|reference" />
        <!-- 右文字hint颜色，默认gray -->
        <attr name="itemTextRightHintColor" format="reference|color" />
        <!-- 右文字hint大小，默认同右文字大小 -->
        <attr name="itemTextRightHintSize" format="dimension" />
    </declare-styleable>
    <!-- 自定义进度条 -->
    <declare-styleable name="CustomProgressBar">
        <attr name="ringradius" format="dimension" />
        <attr name="ringWidth" format="dimension" />
        <attr name="ringColor" format="color" />
    </declare-styleable>
    <declare-styleable name="StickyScrollView">
        <attr name="stuckShadowHeight" format="dimension" />
        <attr name="stuckShadowDrawable" format="reference" />
    </declare-styleable>
    
    <!-- 周边游倒计时器 -->
    <declare-styleable name="TravelCountDownView">
        <attr name="defaultStyle" format="boolean" />
    </declare-styleable>

    <!-- Vacation start -->
    <declare-styleable name="MaxHeightListView">
        <attr name="maxHeight" format="dimension" />
    </declare-styleable>
    <!-- 比例ImageView -->
    <declare-styleable name="RatioLayout">
        <attr name="ratioReference">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="widthRatio" format="float" />
        <attr name="heightRatio" format="float" />
    </declare-styleable>
    <!-- Vacation end -->

    <!-- ExpandableTextView -->
    <declare-styleable name="ExpandableTextView">
        <attr name="maxCollapsedLines" format="integer"/>
        <attr name="animDuration" format="integer"/>
        <attr name="animAlphaStart" format="float"/>
        <attr name="expandDrawable" format="reference"/>
        <attr name="collapseDrawable" format="reference"/>
    </declare-styleable>
    
    <!-- 雷达扫描 -->
     <declare-styleable name="RippleBackground">
        <attr name="rb_color" format="color" />
        <attr name="rb_strokeWidth" format="dimension"/>
        <attr name="rb_radius" format="dimension"/>
        <attr name="rb_duration" format="integer"/>
        <attr name="rb_rippleAmount" format="integer"/>
        <attr name="rb_scale" format="float"/>
        <attr name="rb_type" format="enum">
            <enum name="fillRipple" value="0"/>
            <enum name="strokeRipple" value="1"/>
        </attr>
    </declare-styleable>
    
    <declare-styleable name="FlowLayout">
        <attr name="horizontalSpacing" format="dimension" />
        <attr name="verticalSpacing" format="dimension" />
         <attr name="numColumns" format="integer" />
    </declare-styleable>
    <declare-styleable name="FlowLayout_LayoutParams">
    	<attr name="layout_breakLine" format="boolean" />
        <attr name="layout_horizontalSpacing" format="dimension" />
    </declare-styleable>
    <declare-styleable name="TextViewMultilineEllipse">
        <attr name="android:text" />
        <attr name="android:textColor" />
        <attr name="android:textSize" />
        <attr name="android:maxLines" />
    </declare-styleable>
    <declare-styleable name="TriangleView">
        <attr name="tr_direction" format="enum">
            <enum name="left" value="0"/>
            <enum name="up" value="1"/>
            <enum name="right" value="2"/>
            <enum name="down" value="3"/>
            <enum name="tag" value="4"/>
            <enum name="top_right" value="5"/>
        </attr>
        <attr name="tr_color" format="color"/>
    </declare-styleable>
    <declare-styleable name="SwipeMenu">
        <attr name="anim_duration" format="integer" />
    </declare-styleable>
    <declare-styleable name="AspectRatioView">
        <attr name="aspectRatio" format="float" />
    </declare-styleable>

    <declare-styleable name="DividerPadding">
        <attr name="dividerPaddingLeft" format="dimension" />
        <attr name="dividerPaddingRight" format="dimension" />
        <attr name="dividerPaddingTop" format="dimension" />
        <attr name="dividerPaddingBottom" format="dimension" />
    </declare-styleable>
</resources>